def is_prime(func):
    def wrapper(*args, **kwargs):
        result = func(*args, **kwargs)
        for i in range(2, result):
            if result % i == 0:
                return  "Составное"
                break
        else:
            return f'{result} - Простое число'

    return wrapper


@is_prime
def sum_three(a, b, c):
    d = a + b + c
    return d

print(sum_three(11111111,2,4))
